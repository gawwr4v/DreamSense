<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="homeVM" type="com.home.app.modules.home.data.viewmodel.HomeVM"/>
    </data>
    <LinearLayout android:id="@+id/linearHome"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_48pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowhello"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_27pxh"
                android:layout_marginStart="@dimen/_27pxh"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnhello"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtHello"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.03"
                        android:lineSpacingExtra="@dimen/_13pxh"
                        android:singleLine="true"
                        android:text="@{homeVM.homeModel.txtHello}"
                        style="@style/txtSatoshimedium35" tools:text="@string/lbl_hello"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtErin"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.05"
                        android:lineSpacingExtra="@dimen/_7pxh"
                        android:singleLine="true"
                        android:text="@{homeVM.homeModel.txtErin}"
                        style="@style/txtSatoshiregular17" tools:text="@string/lbl_erin"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageSearchOne"
                    android:layout_height="@dimen/_30pxh"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:layout_marginTop="@dimen/_18pxv"
                    android:layout_width="@dimen/_30pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_search"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_search"/>
                <ImageView android:id="@+id/imageNotifications"
                    android:layout_height="@dimen/_30pxh"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_18pxv"
                    android:layout_width="@dimen/_30pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_notifications"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_notifications"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearColumnmoncounte"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowmoncounter"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_17pxh"
                android:layout_marginStart="@dimen/_17pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:paddingBottom="@dimen/_19pxv"
                android:paddingEnd="@dimen/_18pxh"
                android:paddingStart="@dimen/_18pxh"
                android:paddingTop="@dimen/_19pxv" style="@style/groupStyleblue_gray_200cornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMonCounter"
                    android:layout_gravity="bottom"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.03"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:singleLine="true"
                    android:text="@{homeVM.homeModel.txtMonCounter}"
                    style="@style/txtSatoshimedium15" tools:text="@string/lbl_mon_24"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtJanCounter"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_155pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.03"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:singleLine="true"
                    android:text="@{homeVM.homeModel.txtJanCounter}"
                    style="@style/txtSatoshimedium15" tools:text="@string/lbl_jan_2024"/>
            </LinearLayout>
            <View
                android:background="@drawable/rectangle_gradient_s_gray_50_c_blue_gray_200_e_gray_50"
                android:id="@+id/lineLineoneOne"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginEnd="@dimen/_35pxh"
                android:layout_marginStart="@dimen/_28pxh"
                android:layout_marginTop="@dimen/_18pxv" android:layout_width="@dimen/_297pxh"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtRecents"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_27pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="wrap_content"
                android:letterSpacing="0.03"
                android:lineSpacingExtra="@dimen/_11pxh"
                android:singleLine="true"
                android:text="@{homeVM.homeModel.txtRecents}"
                style="@style/txtSatoshimedium29" tools:text="@string/lbl_recents"/>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="end" android:id="@+id/recyclerHome"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_3pxh"
                android:layout_marginStart="@dimen/_28pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_16pxh}" tools:itemCount="5" tools:listitem="@layout/row_home"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumndescripti"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_31pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:paddingEnd="@dimen/_15pxh"
                android:paddingStart="@dimen/_15pxh" style="@style/groupStyleblue_gray_1">
                <LinearLayout android:id="@+id/linearRowmianine"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageCircleimage"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_33pxh"
                        android:layout_width="@dimen/_33pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_rectangle_16}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle_16"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnmianine"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtMianine"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.03"
                            android:lineSpacingExtra="@dimen/_5pxh"
                            android:singleLine="true"
                            android:text="@{homeVM.homeModel.txtMianine}"
                            style="@style/txtSatoshimedium14" tools:text="@string/lbl_mia_09"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtViewtoread"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.03"
                            android:lineSpacingExtra="@dimen/_5pxh"
                            android:singleLine="true"
                            android:text="@{homeVM.homeModel.txtViewtoread}"
                            style="@style/txtSatoshilight12" tools:text="@string/msg_view_to_read_more"/>
                    </LinearLayout>
                </LinearLayout>
                <TextView android:gravity="center_horizontal"
                    android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_12pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.05"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:text="@{homeVM.homeModel.txtDescription}"
                    style="@style/txtSatoshilight14" tools:text="@string/msg_elara_found_herself"/>
            </LinearLayout>
            <FrameLayout android:gravity="center_horizontal"
                android:id="@+id/frameStackhomeOne"
                android:layout_height="@dimen/_144pxv"
                android:layout_marginTop="@dimen/_6pxv" android:layout_width="match_parent">
                <LinearLayout android:id="@+id/linearColumnhomeOne"
                    android:layout_gravity="bottom|center"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:paddingBottom="@dimen/_10pxv"
                    android:paddingEnd="@dimen/_39pxh"
                    android:paddingStart="@dimen/_39pxh"
                    android:paddingTop="@dimen/_10pxv" style="@style/groupStyleblue_gray_200">
                    <LinearLayout android:gravity="center_vertical"
                        android:id="@+id/linearRowhomeOne"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageHomeOne"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_40pxh"
                            android:layout_width="@dimen/_40pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_home"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_home"/>
                        <View
                            android:background="@drawable/img_ellipse_1"
                            android:id="@+id/viewView"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_33pxh"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_marginStart="@dimen/_208pxh"
                            android:layout_marginTop="@dimen/_3pxv" android:layout_width="@dimen/_33pxh"/>
                    </LinearLayout>
                    <View
                        android:background="@drawable/rectangle_bg_gray_900_radius_2"
                        android:id="@+id/lineHandleOne"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_4pxv"
                        android:layout_marginTop="@dimen/_5pxv" android:layout_width="@dimen/_108pxh"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowfavoritebord"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageFavoriteborder"
                        android:layout_height="@dimen/_28pxh"
                        android:layout_marginBottom="@dimen/_76pxv"
                        android:layout_width="@dimen/_28pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_favorite_border"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_favorite_border"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLikesCounter"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_78pxv"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.05"
                        android:lineSpacingExtra="@dimen/_5pxh"
                        android:singleLine="true"
                        android:text="@{homeVM.homeModel.txtLikesCounter}"
                        style="@style/txtSatoshimedium13" tools:text="@string/lbl_13_likes"/>
                    <FrameLayout android:id="@+id/frameStackvectorOne"
                        android:layout_height="@dimen/_100pxv"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="@dimen/_100pxh"
                        android:paddingHorizontal="@dimen/_33pxh"
                        android:paddingVertical="@dimen/_33pxv" style="@style/groupStylegray_50cornerRadius">
                        <ImageView android:id="@+id/imageVectorOne"
                            android:layout_gravity="top|center"
                            android:layout_height="@dimen/_32pxh"
                            android:layout_width="@dimen/_32pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                    </FrameLayout>
                    <ImageView android:id="@+id/imageModecommentOne"
                        android:layout_height="@dimen/_28pxh"
                        android:layout_marginBottom="@dimen/_75pxv"
                        android:layout_marginStart="@dimen/_71pxh"
                        android:layout_width="@dimen/_28pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_mode_comment"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_mode_comment"/>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
